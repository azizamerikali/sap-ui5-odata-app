{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "webapp/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/sap/(.*)",
            "dest": "/api/sap/[...path].js"
        },
        {
            "src": "/(.*)",
            "dest": "/webapp/$1"
        }
    ]
}