{
    "welcomeFile": "/sapuidemoodata/index.html",
    "authenticationMethod": "route",
    "websockets": {
        "enabled": true
    },
    "routes": [
        {
            "source": "^/sap\\((.*)\\)/(.*)$",
            "target": "/sap($1)/$2",
            "destination": "MBP",
            "authenticationType": "none",
            "csrfProtection": false
        },
        {
            "source": "^/sap/(.*)$",
            "target": "/sap/$1",
            "destination": "MBP",
            "authenticationType": "none",
            "csrfProtection": false
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "xsuaa"
        }
    ]
}