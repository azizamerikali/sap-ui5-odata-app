<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="{title}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpConfirm"
		cancel=".onValueHelpCancel"
		items="{
			path: '/SummarySet',
			sorter: { path: 'key', descending: false }
		}">
		<StandardListItem
			title="{}"
			description="{}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>
