{
    "name": "sap-odata-proxy",
    "version": "1.0.0",
    "description": "Proxy server for SAP OData",
    "main": "proxy.js",
    "scripts": {
        "proxy": "node proxy.js",
        "start": "concurrently \"npm run proxy\" \"npx http-server ./webapp -p 8080 --cors\"",
        "serve": "npx http-server ./webapp -p 8080 --cors -o",
        "build:cf": "ui5 build --clean-dest --dest dist"
    },
    "dependencies": {
        "express": "^4.18.2",
        "node-fetch": "^2.7.0",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "@ui5/cli": "^3.0.0"
    }
}