{
    "name": "sap-odata-proxy",
    "version": "1.0.0",
    "description": "Proxy server for SAP OData",
    "main": "proxy.js",
    "scripts": {
        "proxy": "node proxy.js",
        "start": "node node_modules/@sap/approuter/approuter.js",
        "start-local": "node proxy.js",
        "serve": "npx http-server ./webapp -p 8080 --cors -o",
        "build:cf": "ui5 build --clean-dest --dest dist",
        "build:zip": "npm run build:cf && powershell -Command \"Compress-Archive -Path dist\\* -DestinationPath sap-ui5-odata-app.zip -Force\""
    },
    "dependencies": {
        "@sap/approuter": "^20.8.8",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "node-fetch": "^2.7.0"
    },
    "devDependencies": {
        "@ui5/cli": "^3.0.0",
        "concurrently": "^8.2.2"
    }
}